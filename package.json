{
  "name": "igcse-toolkit",
  "version": "2.0.0",
  "description": "An app to help you get good grades",
  "main": "main.js",
  "author": "TheComputerM",
  "scripts": {
    "start": "electron .",
    "package-win-32": "electron-packager . IGCSE-Toolkit --overwrite --no-asar --platform=win32 --icon=readme/programIcon.ico --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=TheComputerM --version-string.FileDescription=Study --version-string.ProductName=\"IGCSE Toolkit\"",
    "package-win-64": "electron-packager . IGCSE-Toolkit --overwrite --no-asar --platform=win32 --icon=readme/programIcon.ico --arch=ia64 --prune=true --out=release-builds --version-string.CompanyName=TheComputerM --version-string.FileDescription=Study --version-string.ProductName=\"IGCSE Toolkit\""
  },
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.9",
    "hammerjs": "^2.0.8",
    "ionicons": "^4.4.3",
    "jquery": "^3.3.1",
    "materialize-css": "^1.0.0"
  },
  "devDependencies": {
    "electron-packager": "^12.1.2"
  }
}
