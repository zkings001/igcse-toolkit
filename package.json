  "description": "An app to help you get good grades",
  "main": "main.js",
  "author": "TheComputerM",
  "build": {
    "appId": "thecomputerm.id",
    "mac": {
      "category": "your.app.category.type"
    }
  },
  "scripts": {
    "start": "gulp sass && electron .",
